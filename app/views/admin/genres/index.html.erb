    <h3>ジャンル一覧・追加</h3>
      <%=form_with model: @genre,url: admin_genres_path, method: :post do |f| %>
      <div class="form-group">
        <%= f.label :"ジャンル名", class: "mr-3" %>
        <%= f.text_field :name, placeholder: "ジャンル名", class: "mr-5" %>
        <%= f.submit'新規登録',class: "btn btn-primary" %>
      </div>
      <% end %>

  <table class="table">
    <thead>
      <tr>
        <th>
          ジャンル
        </th>
      </tr>
    </thead>
    <tbody>
      <% @genres.each do |genre| %>
      <tr>
        <td>
          <%= genre.name %>
        </td>
        <td>
          <%= link_to "編集する", edit_admin_genre_path(genre.id), class: "btn-success" %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>